*:/op [$] = >>>
    if(or(has_permission('rccmd.op'), has_permission('bukkit.command.op.give')),
        run('/op '.$),
        die(@no_permission_error)
    )
<<<

*:/deop [$] = >>>
    if(or(has_permission('rccmd.deop'), has_permission('bukkit.command.op.take')),
        run('/deop '.$),
        die(@no_permission_error)
    )
<<<

*:/pl [$] = >>>
    call_alias('/plugins '.$)
<<<

*:/mods [$] = >>>
    call_alias('/plugins '.$)
<<<

*:/plugins [$] = >>>
    if(or(has_permission('rccmd.plugins'), has_permission('bukkit.command.plugins')),
        run('/plugins '.$),
        die(@no_permission_error)
    )
<<<

*:/help [$] = >>>
    if(or(has_permission('rccmd.help'), has_permission('bukkit.command.help')),
        call_alias('/rccmdhelp '.$),
        die(@no_permission_error)
    )
<<<


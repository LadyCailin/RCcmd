*:/op [$] = >>>
    include('lib.library/config.msa')
    if(equals(@enable_op, 'true'),
        run('/op '.$),
        die(@no_permission_error)
    )
<<<

*:/deop [$] = >>>
    include('lib.library/config.msa')
    if(equals(@enable_deop, 'true'),
        run('/deop '.$),
        die(@no_permission_error)
    )
<<<

*:/plugins [$] = >>>
    include('lib.library/config.msa')
    if(equals(@enable_plugins, 'true'),
        run('/plugins '.$),
        die(@no_permission_error)
    )
<<<

*:/pl [$] = >>>
    call_alias('/plugins '.$)
<<<

*:/mods [$] = >>>
    call_alias('/plugins '.$)
<<<

*:/kill [$player='<me>']= >>>
    assign(@player, $player)
    include('lib.library/config.msa')
    assign(@command, array_get(reg_split(' ', get_cmd()), 1))
    if(not(equals(@enabled_commands[@command], 'true')),
        run(@command.$)
        die()
    )
    _checkforperm('kill')
    if(equals(@player, '<me>'),
        assign(@player, player()),
        true
    )
    kill(@player)
<<<
